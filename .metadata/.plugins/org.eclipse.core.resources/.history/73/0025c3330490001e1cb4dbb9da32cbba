# This proper file contains Lot of build in properties 
# We just have to write values for that properties 
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver
spring.datasource.url=jdbc:mysql://localhost:3306/sms_jpa_db1
spring.datasource.username=root
spring.datasource.password=root


spring.jpa.hibernate.ddl.auto=update